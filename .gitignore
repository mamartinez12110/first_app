# Gemfile.lock

# Ignore all Docfiles.
/doc/api
/doc/app

# Ignore all swpfiles.
*.swp

# Ignore all DS_Storefiles.
.DS_Store

# Ignore all rootfiles.
*~

# Ignore other unneeded files.
database.yml
doc/
*.swp
*~
.project
.DS_Store
.idea
.secret